    <TextView
        android:id="@+id/forthButtonNameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="38dp"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/forthItemButton"
        app:layout_constraintEnd_toEndOf="@+id/forthItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/forthButtonImageView"
        app:layout_constraintTop_toTopOf="@+id/forthItemButton"
        app:layout_constraintVertical_bias="0.282" />

    <TextView
        android:id="@+id/secondButtonNameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/thirdItemButton"
        app:layout_constraintEnd_toEndOf="@+id/secondItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/secondButtonImageView"
        app:layout_constraintTop_toBottomOf="@+id/firstItemButton"
        app:layout_constraintVertical_bias="0.071" />

    <TextView
        android:id="@+id/firstButtonDescriptionText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/firstItemButton"
        app:layout_constraintEnd_toEndOf="@+id/firstItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/firstButtonImageView"
        app:layout_constraintTop_toTopOf="@+id/firstItemButton"
        app:layout_constraintVertical_bias="0.494" />

    <TextView
        android:id="@+id/secondButtonDescriptionText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        app:layout_constraintBottom_toTopOf="@+id/thirdItemButton"
        app:layout_constraintEnd_toEndOf="@+id/secondItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/secondButtonImageView"
        app:layout_constraintTop_toBottomOf="@+id/firstItemButton"
        app:layout_constraintVertical_bias="0.525" />

    <TextView
        android:id="@+id/thirdButtonDescriptionText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/thirdItemButton"
        app:layout_constraintEnd_toEndOf="@+id/thirdItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/thirdButtonImageView"
        app:layout_constraintTop_toTopOf="@+id/thirdItemButton"
        app:layout_constraintVertical_bias="0.505" />

    <TextView
        android:id="@+id/forthButtonDescriptionText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/forthItemButton"
        app:layout_constraintEnd_toEndOf="@+id/forthItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/forthButtonImageView"
        app:layout_constraintTop_toBottomOf="@+id/thirdItemButton"
        app:layout_constraintVertical_bias="0.568" />

