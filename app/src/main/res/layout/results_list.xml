<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/Green_Bottom"
        android:layout_width="420dp"
        android:layout_height="70dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:src="@drawable/result_bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/result_bottom" />

    <ImageView
        android:id="@+id/Green_top"
        android:layout_width="559dp"
        android:layout_height="227dp"
        android:src="@drawable/result_top"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/result_top" />


    <TextView
        android:id="@+id/emptyListText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="רשימה זו ריקה"
        android:textColor="#0C0A0B"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.449" />

    <ImageView
        android:id="@+id/firstItemButton"
        android:layout_width="376dp"
        android:layout_height="116dp"
        android:onClick="contributionButton1"
        android:src="@drawable/result_slot"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.542"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.26"
        app:srcCompat="@drawable/result_slot" />

    <ImageView
        android:id="@+id/thirdItemButton"
        android:layout_width="376dp"
        android:layout_height="116dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:onClick="contributionButton3"
        android:src="@drawable/result_slot"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secondItemButton"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/result_slot" />

    <ImageView
        android:id="@+id/forthItemButton"
        android:layout_width="376dp"
        android:layout_height="116dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:onClick="contributionButton4"
        android:src="@drawable/result_slot"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/thirdItemButton"
        app:layout_constraintVertical_bias="0.042" />

    <ImageView
        android:id="@+id/nextPageButton"
        android:layout_width="152dp"
        android:layout_height="54dp"
        android:onClick="nextPage"
        android:src="@drawable/move_forward"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.064"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/move_forward" />

    <ImageView
        android:id="@+id/previousPageButton"
        android:layout_width="152dp"
        android:layout_height="54dp"
        android:onClick="previousPage"
        android:src="@drawable/move_back"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/move_back" />

    <EditText
        android:id="@+id/searchText"
        android:layout_width="254dp"
        android:layout_height="58dp"
        android:ems="10"
        android:hint="חיפוש"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.757"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.104" />

    <Button
        android:id="@+id/signIN_button5"
        android:layout_width="102dp"
        android:layout_height="48dp"
        android:background="@drawable/button_login_background"
        android:onClick="searchButton"
        android:text="חיפוש"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#FFFFFF"
        android:textStyle="bold"
        android:visibility="visible"
        app:backgroundTint="#F44336"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/searchText"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.102" />

    <ImageView
        android:id="@+id/secondItemButton"
        android:layout_width="376dp"
        android:layout_height="116dp"
        android:layout_marginEnd="1dp"
        android:layout_marginRight="1dp"
        android:onClick="contributionButton2"
        android:src="@drawable/result_slot"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstItemButton"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/result_slot" />

    <ImageView
        android:id="@+id/my_help"
        android:layout_width="265dp"
        android:layout_height="72dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="7dp"
        android:layout_marginRight="7dp"
        android:layout_marginBottom="612dp"
        android:src="@drawable/ronen2"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.436"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ronen2" />

    <ImageView
        android:id="@+id/delivery_results"
        android:layout_width="276dp"
        android:layout_height="72dp"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginEnd="14dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="527dp"
        android:src="@drawable/my_need_delivery"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/my_need_delivery" />

    <ImageView
        android:id="@+id/my_contributions"
        android:layout_width="265dp"
        android:layout_height="72dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="476dp"
        android:src="@drawable/my_assigned_tome"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.532"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.038"
        app:srcCompat="@drawable/my_assigned_tome" />

    <ImageView
        android:id="@+id/givers_results"
        android:layout_width="265dp"
        android:layout_height="72dp"
        android:layout_marginBottom="461dp"
        android:src="@drawable/my_available_givvers"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/my_available_givvers" />

    <ImageView
        android:id="@+id/needs_help_results"
        android:layout_width="265dp"
        android:layout_height="72dp"
        android:src="@drawable/my_posts_forhelp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/my_posts_forhelp" />

    <ImageView
        android:id="@+id/firstButtonImageView"
        android:layout_width="128dp"
        android:layout_height="97dp"
        android:layout_marginBottom="7dp"
        app:layout_constraintBottom_toTopOf="@+id/secondItemButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/firstItemButton"
        app:layout_constraintTop_toTopOf="@+id/firstItemButton"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/secondButtonImageView"
        android:layout_width="128dp"
        android:layout_height="97dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toTopOf="@+id/thirdItemButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/secondItemButton"
        app:layout_constraintTop_toTopOf="@+id/secondItemButton"
        app:layout_constraintVertical_bias="0.4"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/thirdButtonImageView"
        android:layout_width="128dp"
        android:layout_height="97dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/thirdItemButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/thirdItemButton"
        app:layout_constraintTop_toTopOf="@+id/thirdItemButton"
        app:layout_constraintVertical_bias="0.4"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/forthButtonImageView"
        android:layout_width="128dp"
        android:layout_height="97dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toBottomOf="@+id/forthItemButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.041"
        app:layout_constraintStart_toStartOf="@+id/forthItemButton"
        app:layout_constraintTop_toTopOf="@+id/forthItemButton"
        app:layout_constraintVertical_bias="1.0"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/searchTextError"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.722"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchText"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/firstButtonNameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/firstItemButton"
        app:layout_constraintEnd_toEndOf="@+id/firstItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/firstButtonImageView"
        app:layout_constraintTop_toTopOf="@+id/firstItemButton"
        app:layout_constraintVertical_bias="0.071" />

    <TextView
        android:id="@+id/thirdButtonNameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/thirdItemButton"
        app:layout_constraintEnd_toEndOf="@+id/thirdItemButton"
        app:layout_constraintHorizontal_bias="0.841"
        app:layout_constraintStart_toStartOf="@+id/thirdButtonImageView"
        app:layout_constraintTop_toTopOf="@+id/thirdItemButton"
